{
  "name": "pollard",
  "version": "0.3.2",
  "description": "dj playlist creator frontend",
  "repository": {
    "type": "git",
    "url": "git://github.com/spencerliechty/pollard.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "./bin/buildenv.sh && node server.js",
    "build-env-dev": "./bin/buildenv.sh",
    "build": "webpack --progress --colors",
    "build-min-dev": "./bin/buildenv.sh && webpack --config webpackmin.config.js --progress --colors",
    "build-min": "webpack --config webpackmin.config.js --progress --colors",
    "test": "NODE_ENV=test mocha --compilers js:babel/register --recursive",
    "test:watch": "NODE_ENV=test mocha --compilers js:babel/register --recursive --watch",
    "test:cov": "babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha -- --recursive"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.1.2",
    "exports-loader": "^0.6.2",
    "history": "^1.12.6",
    "immutable": "^3.7.5",
    "imports-loader": "^0.6.5",
    "moment-timezone": "^0.5.2",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.4",
    "redux-router": "^1.0.0-beta3",
    "socket.io-client": "^1.3.7",
    "whatwg-fetch": "^0.10.0"
  },
  "devDependencies": {
    "babel": "^5.8.12",
    "babel-core": "^5.6.18",
    "babel-loader": "^5.1.4",
    "expect": "^1.8.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.2",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.2.7",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  }
}
